{
    "name": "lebenlabs/simplecms",
    "description": "Package para CMS simplificado",
    "license": "license",
    "authors": [
        {
            "name": "Lebenlabs",
            "email": "devops@lebenlabs.com",
            "homepage": "https://github.com/leamasuero/simplecms"
        }
    ],
    "homepage": "https://github.com/leamasuero/simplecms",
    "keywords": ["Laravel", "Lebenlabs", "SimpleCMS"],
    "require": {
        "illuminate/support": "~5",
        "gedmo/doctrine-extensions": "^2.4",
        "laravel-doctrine/extensions": "1.0.*",
        "laravel-doctrine/migrations": "1.1.*",
        "laravel-doctrine/orm": "1.4.*",
        "intervention/image": "^2.4",
        "laracasts/flash": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Lebenlabs\\SimpleCMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lebenlabs\\SimpleCMS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lebenlabs\\SimpleCMS\\SimpleCMSServiceProvider"
            ],
            "aliases": {
                "SimpleCMS": "Lebenlabs\\SimpleCMS\\Facades\\SimpleCMS"
            }
        }
    }
}
