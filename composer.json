{
    "name": "lebenlabs/simplecms",
    "description": "Package para CMS simplificado",
    "authors": [
        {
            "name": "Lebenlabs",
            "email": "devops@lebenlabs.com",
            "homepage": "https://github.com/leamasuero/simplecms"
        }
    ],
    "homepage": "https://github.com/leamasuero/simplecms",
    "keywords": ["Laravel", "Lebenlabs", "SimpleCMS"],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.0|^6.0|^7.0",
        "pagerfanta/pagerfanta": "^2.1",
        "laracasts/flash": "^3.0",
        "lebenlabs/html": "1.0.*",
        "lebenlabs/simplestorage": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Lebenlabs\\SimpleCMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lebenlabs\\SimpleCMS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lebenlabs\\SimpleCMS\\SimpleCMSServiceProvider"
            ],
            "aliases": {
                "SimpleCMS": "Lebenlabs\\SimpleCMS\\Facades\\SimpleCMS"
            }
        }
    }
}
